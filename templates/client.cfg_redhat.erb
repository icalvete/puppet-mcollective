topicprefix = /topic/
main_collective = mcollective
collectives = mcollective
libdir = /usr/libexec/mcollective
logger_type = console
loglevel = warn

# Plugins
securityprovider = psk
plugin.psk = unset

connector = stomp
plugin.stomp.host = <%= scope.lookupvar('mcollective::client::stomp_host') %>
plugin.stomp.port = <%= scope.lookupvar('mcollective::client::stomp_port') %>
plugin.stomp.user = <%= scope.lookupvar('mcollective::client::stomp_user') %>
plugin.stomp.password = <%= scope.lookupvar('mcollective::client::stomp_pass') %>

# Facts
factsource = facter
#plugin.yaml = /etc/mcollective/facts.yaml
